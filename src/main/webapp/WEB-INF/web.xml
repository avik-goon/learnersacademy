<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
      <servlet-name>login</servlet-name>
      <servlet-class>com.learnersacademy.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>main.webapp.com.learnersacademy.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>master_class_list</servlet-name>
        <servlet-class>main.webapp.com.learnersacademy.MasterClassList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>master_class_list</servlet-name>
        <url-pattern>/master_class_list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>master_subject_list</servlet-name>
        <servlet-class>main.webapp.com.learnersacademy.MasterSubjectCreator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>master_subject_list</servlet-name>
        <url-pattern>/master_subject_list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>master_student_list</servlet-name>
        <servlet-class>main.webapp.com.learnersacademy.StudentCreator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>master_student_list</servlet-name>
        <url-pattern>/master_student_list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>master_teacher_list</servlet-name>
        <servlet-class>main.webapp.com.learnersacademy.TeacherCreator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>master_teacher_list</servlet-name>
        <url-pattern>/master_teacher_list</url-pattern>
    </servlet-mapping>


</web-app>
